{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@latest"
        },
        "api/assets.php": {
            "runtime": "vercel-php@latest"
        }
    },

    "routes": [
        {
            "src": "/build/assets/(.*)",
            "dest": "/api/assets.php?assets=$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Laravel Vercel",
        "APP_DEBUG": "false",
        "APP_KEY": "APP_KEY=base64:u5L5AQ5WJrqkrUqul/0eKvJMTh+tmphRBuubs9woHG8=",
        "APP_URL": "https://berwisata-v2.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "CACHE_DRIVER": "array",
        "CACHE_STORE": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "VIEW_COMPILED_PATH": "/tmp"
    },
    "build": {
        "installCommand": "composer install --no-dev --optimize-autoloader"
    }

}
